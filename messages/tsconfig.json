        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": false,
        "preserveConstEnums": true,
        "sourceMap": true,
        "target": "es6",
        "outDir": "js",
        "types": [ "node" ]
    },
    "include": [
        "dialogs*",
        "services*",
        "index.ts",
        "mockResponse.ts"
    ],
    "exclude": [
        "**/node_modules",
        "**/*.d.ts"
    ]
}
